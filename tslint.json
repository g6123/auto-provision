{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "no-console": {
      "options": [],
      "severity": "warn"
    },
    "interface-over-type-literal": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "object-literal-sort-keys": false,
    "trailing-comma": [
      true,
      "always"
    ],
    "semicolon": [
      true,
      "never"
    ],
    "eofline": true
  },
  "rulesDirectory": []
}
