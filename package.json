{
  "name": "di-helper",
  "version": "1.0.0",
  "description": "A concise JavaScript dependency injector",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/g6123/auto-provision",
  "author": "cumul <gg6123@naver.com>",
  "license": "MIT",
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run clean && tsc --declaration",
    "test": "nyc --extension .ts --require ts-node/register mocha test/**/*.ts",
    "coverage": "nyc --extension .ts --require ts-node/register --reporter html --report-dir .nyc_output/coverage report test/**/*.ts && opener .nyc_output/coverage/index.html",
    "clean": "del-cli lib/**/* !**/.gitignore"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/chai-spies": "^0.0.0",
    "@types/mocha": "^5.0.0",
    "chai": "^4.1.2",
    "chai-spies": "^1.0.0",
    "del-cli": "^1.1.0",
    "mocha": "^5.0.5",
    "nyc": "^11.6.0",
    "opener": "^1.4.3",
    "ts-node": "^5.0.1",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.20",
    "@types/lodash": "^4.14.106",
    "bluebird": "^3.5.1",
    "lodash": "^4.17.5"
  }
}
